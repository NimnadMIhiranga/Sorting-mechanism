





int isObstaclePin = 12;  // This is our input pin
int isObstacle = LOW;  // HIGH MEANS NO OBSTACLE
int motorpin = 11;












void setup(){
  pinMode(12, INPUT);
  pinMode(11, OUTPUT);
  
  





}

















void IR(){
  isObstacle = digitalRead(isObstaclePin);
  if (isObstacle == HIGH){
  return HIGH;
  }
  else{
  return LOW;  
  }
 delay(200)



}

void L298(){
 digitalWrite(motorpin, HIGH);
}


